{
  "name": "cmc-mod-manager",
  "version": "3.2.0",
  "description": "A Mod Manager For SSBC CMC+ v8 Made With Electron",
  "main": ".webpack/main",
  "icon": "./gb/icon.png",
  "scripts": {
    "start": "electron-forge start",
    "gen-langs": "node lang/gen-langs.mjs",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make-win": "electron-forge make --platform win32 --targets @electron-forge/maker-zip",
    "make-win32": "electron-forge make --platform win32 --targets @electron-forge/maker-zip --arch=ia32",
    "make-linux-all": "electron-forge make --platform linux --targets @electron-forge/maker-deb,@electron-forge/maker-rpm,@electron-forge/maker-zip",
    "make-linux": "electron-forge make --platform linux --targets @electron-forge/maker-zip",
    "make-deb": "electron-forge make --platform linux --targets @electron-forge/maker-deb",
    "make-rpm": "electron-forge make --platform linux --targets @electron-forge/maker-rpm",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "Inferno214221",
    "email": "inferno214221@gmail.com"
  },
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/plugin-webpack": "^7.2.0",
    "@stylistic/eslint-plugin-js": "^1.5.1",
    "@stylistic/eslint-plugin-jsx": "^1.5.1",
    "@stylistic/eslint-plugin-ts": "^1.5.1",
    "@types/fs-extra": "^11.0.4",
    "@types/ini": "^4.1.0",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/request": "^2.48.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "css-loader": "^6.0.0",
    "electron": "30.0.0",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "node-loader": "^2.0.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0"
  },
  "dependencies": {
    "child_process": "^1.0.2",
    "electron-squirrel-startup": "^1.0.0",
    "extract-zip": "^2.0.1",
    "fs-extra": "^11.2.0",
    "ini": "^4.1.1",
    "node-unrar-js": "^2.0.2",
    "path": "^0.12.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "request": "^2.88.2",
    "semver": "^7.5.4"
  }
}
